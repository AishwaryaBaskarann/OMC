<!DOCTYPE html>
<html lang="en">

<%- include('./partials/head.ejs') %>

  <body>

    <%- include('./partials/nav.ejs') %>

      <div class="branding">OMC</div>
      <div class="form-div">
        <form method="POST" action="/success">
          <div class="form-group">
            <label>Name</label>
            <input type="text" class="form-control" id="exampleFormControlInput1"
              placeholder="Please enter your name" />
          </div>
          <div class="form-group">
            <label>Age</label>
            <input type="number" class="form-control" id="exampleFormControlInput2" placeholder="" />
          </div>
          <div class="form-group">
            <label for="exampleFormControlSelect3">Sex</label>
            <select class="form-control" id="exampleFormControlSelect4">
              <option>Male</opoktion>
              <option>Female</option>
              <option>Others</option>
            </select>
          </div>
          <div class="form-group">
            <label>Phone:</label>
            <input type="number" class="form-control" id="exampleFormControlInput5" placeholder="" maxlength="10" />
          </div>
          <div class="form-group">
            <label>Password</label>
            <input type="password" class="form-control" id="exampleFormControlInput6"
              placeholder="Set your password here" />
          </div>
          <div class="form-group">
            <label>Address</label>
            <input type="text" class="form-control" id="exampleFormControlInput7"
              placeholder="Enter your address here" />
          </div>
          <button type="submit" class="btn btn-primary mb-2 submit">
            Register
          </button>
        </form>
      </div>
  </body>

</html>